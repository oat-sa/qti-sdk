                <xs:attributeGroup ref="min-choices.PositionObjectInteraction.Attr"/>
                <xs:attributeGroup ref="max-choices.PositionObjectInteraction.Attr"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

