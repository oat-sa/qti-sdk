                <xs:attributeGroup ref="title.EndAttemptInteraction.Attr"/>
                <xs:attributeGroup ref="countAttempt.EndAttemptInteraction.Attr"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

