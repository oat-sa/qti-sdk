                <xs:element ref="stringMatch" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="repeat" minOccurs="1" maxOccurs="1"/>
                <xs:element ref="roundTo" minOccurs="1" maxOccurs="1"/>
